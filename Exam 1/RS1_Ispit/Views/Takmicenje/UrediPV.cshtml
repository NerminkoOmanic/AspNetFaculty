@model RS1_Ispit_asp.net_core.ViewModels.TakmicenjeRezultatiUrediVM
@{
    ViewData["Title"] = "UrediPV";
}


<form method="post" asp-action="UrediPV" ajax-poziv="da" ajax-rezultat="ajaxDiv" >
    <div class="form-group">
        <input style="visibility: hidden" name="takmicenjeUcesnikId" class="form-control" value="@Model.TakmicenjeUcesnikId"/>
    </div>
    <div class="form-group">
        <label class="control-label">Ucesnik:  </label>
        <span class="control-label">@Model.UcenikOznaka </span>
    </div>
    <div class="form-group">
        <label class="control-label">Bodovi: </label>
        <input type="number" name="bodovi" class="form-control" placeholder="@Model.Bodovi"/>
    </div>
    <div class="form-group">
        <input type="submit" value="Snimi" class="btn btn-default"/>
    </div>
</form>
